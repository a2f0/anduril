# formats for type status and slaves status_format1 status_format2 input_prompt status_away status_channel status_clock status_hold status_insert status_mail status_mode status_notify status_overwrite status_query status_scrollback status_server status_umode status_window other_status
ac.format.add               status #1
ac.format.add       status_format1 $(bg7)$(fg0) %T [%R] %*%=%@%N%#%S%H%B%Q%A%C%+%I%O%M%F%L %U %>
ac.format.add       status_format2 $(bg7)$(fg0)%W %X %Y %Z %>
ac.format.add         input_prompt > $(ncol)
ac.format.add          status_away  (Away)
ac.format.add       status_channel  %C
ac.format.add         status_clock %T
ac.format.add          status_hold  Held:
ac.format.add        status_insert 
ac.format.add          status_mail  (Mail: %M)
ac.format.add          status_mode  (+%+)
ac.format.add        status_notify  (W: %F)
ac.format.add     status_overwrite  (Overwrite)
ac.format.add         status_query  (Query: %Q)
ac.format.add    status_scrollback  (Scroll)
ac.format.add        status_server %S
ac.format.add         status_umode  (+%#)
ac.format.add        status_window ^^^^^^^^
ac.format.add         other_status $(bg7)$(fg0)[%.] %> [%.]

ac.format.add               status #2
ac.format.add       status_format1 $(dcol1).-[$(ncol)%T$(dcol1)]-[$(hcol1)%R$(dcol1)/$(hcol2)%I%O%K$(hcol2)%A%3$(hcol2)%H%B$(dcol1)]$(dcol1)-[$(hcol1)%*%=%@$(col1)%N%#$(dcol1)]%C%+$(dcol1)]-%>%F$(dcol1)[$(col1)l$(dcol1):$(hcol1)%4$(dcol1)]--.
ac.format.add       status_format2 $(dcol1)| [$(ncol)up$(dcol1):$(hcol1)%2$(dcol1)] %>$(ncol)%D $(dcol1)[$(ncol)$builtin_expando(S)$(dcol1)] $(dcol1)[$(hcol2)a$(dcol1)\($(ncol)%1$(dcol1)\)]  |
ac.format.add         input_prompt $(dcol1)`$(col1)-$(hcol1)> $(ncol)
ac.format.add          status_away A$(dcol2):$(hcol1)
ac.format.add       status_channel  $(dcol1)[$(ncol)%C
ac.format.add         status_clock %T
ac.format.add          status_hold H$(dcol2):$(hcol1)
ac.format.add        status_insert I
ac.format.add          status_mail $(dcol1)[$(hcol1)m$(ncol):$(col2)%M$(dcol1)]
ac.format.add          status_mode $(dcol1)\($(hcol2)+$(col2)%+$(dcol1)\)
ac.format.add        status_notify $(dcol1)[$(hcol1)W$(ncol):%F$(dcol1)]
ac.format.add     status_overwrite O
ac.format.add         status_query $(dcol1)[$(hcol1)Q$(ncol):%Q$(dcol1)]
ac.format.add    status_scrollback S
ac.format.add        status_server %S
ac.format.add         status_umode $(dcol1)\($(hcol1)+$(col1)%#$(dcol1)\)
ac.format.add        status_window $(hcol1)%%
ac.format.add         other_status $(dcol2)[$(hcol2)%.$(dcol2)] -%> [$(hcol2)%.$(dcol2)]

ac.format.add               status #3
ac.format.add       status_format1 $(dcol1)ÚÄ[$(ncol)%T$(dcol1)]Ä[$(hcol1)%R$(dcol1)/$(hcol2)%I%O%K$(hcol2)%A%3$(hcol2)%H%B$(dcol1)]$(dcol1)Ä[$(hcol1)%*%=%@$(col1)%N%#$(dcol1)]%C%+$(dcol1)]Ä%>%F$(dcol1)[$(col1)l$(dcol1):$(hcol1)%4$(dcol1)]ÄÄ¿
ac.format.add       status_format2 $(dcol1)³ [$(ncol)up$(dcol1):$(hcol1)%2$(dcol1)] %>$(ncol)%D $(dcol1)[$(ncol)$builtin_expando(S)$(dcol1)] [$(hcol2)a$(dcol1)\($(ncol)%1$(dcol1)\)]  ³
ac.format.add         input_prompt $(dcol1)À$(col1)Ä$(hcol1)> $(ncol)
ac.format.add          status_away A$(dcol2):$(hcol1)
ac.format.add       status_channel  $(dcol1)[$(ncol)%C
ac.format.add         status_clock %T
